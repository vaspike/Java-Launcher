{
  "name": "java-launcher",
  "displayName": "%extension.displayName%",
  "description": "%extension.description%",
  "version": "1.4.1",
  "publisher": "River",
  "repository": {
    "type": "git",
    "url": "https://github.com/vaspike/Java-Launcher.git"
  },
  "engines": {
    "vscode": "^1.84.0"
  },
  "icon": "images/logo.png",
  "categories": [
    "Debuggers",
    "Other"
  ],
  "keywords": [
    "java",
    "launch",
    "debug",
    "configuration",
    "spring boot",
    "junit",
    "testing"
  ],
  "activationEvents": [
    "onLanguage:java",
    "workspaceContains:**/*.java",
    "onCommand:java-launcher.generateLaunchConfigs"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "java-launcher.generateLaunchConfigs",
        "title": "%command.generateLaunchConfigs.title%",
        "category": "%category.javaLauncher%"
      },
      {
        "command": "java-launcher.scanJavaEntries",
        "title": "%command.scanJavaEntries.title%",
        "category": "%category.javaLauncher%"
      },
      {
        "command": "java-launcher.createAggregatedLaunch",
        "title": "%command.createAggregatedLaunch.title%",
        "icon": "$(plus)",
        "category": "%category.javaLauncher%"
      },
      {
        "command": "java-launcher.manageAggregatedLaunch",
        "title": "%command.manageAggregatedLaunch.title%",
        "icon": "$(settings-gear)",
        "category": "%category.javaLauncher%"
      },
      {
        "command": "java-launcher.executeAggregatedLaunch",
        "title": "%command.executeAggregatedLaunch.title%",
        "category": "%category.javaLauncher%"
      },
      {
        "command": "java-launcher.debugAggregatedLaunch",
        "title": "%command.debugAggregatedLaunch.title%",
        "category": "%category.javaLauncher%"
      },
      {
        "command": "java-launcher.runJavaEntry",
        "title": "%command.runJavaEntry.title%",
        "icon": "$(play)",
        "category": "%category.javaLauncher%"
      },
      {
        "command": "java-launcher.runAggregatedConfig",
        "title": "%command.runAggregatedConfig.title%",
        "icon": "$(play)",
        "category": "%category.javaLauncher%"
      },
      {
        "command": "java-launcher.refreshTreeView",
        "title": "%command.refreshTreeView.title%",
        "icon": "$(refresh)",
        "category": "%category.javaLauncher%"
      },
      {
        "command": "java-launcher.addToAggregatedConfig",
        "title": "%command.addToAggregatedConfig.title%",
        "icon": "$(plus)",
        "category": "%category.javaLauncher%"
      },
      {
        "command": "java-launcher.showAllCommands",
        "title": "%command.showAllCommands.title%",
        "icon": "$(list-unordered)",
        "category": "%category.javaLauncher%"
      },
      {
        "command": "java-launcher.setSpringProfile",
        "title": "%command.setSpringProfile.title%",
        "icon": "$(settings)",
        "category": "%category.javaLauncher%"
      },
      {
        "command": "java-launcher.setAllSpringProfiles",
        "title": "%command.setAllSpringProfiles.title%",
        "icon": "$(settings)",
        "category": "%category.javaLauncher%"
      },
      {
        "command": "java-launcher.setAllJmxRemoteStatus",
        "title": "%command.setAllJmxRemoteStatus.title%",
        "icon": "$(remote-explorer)",
        "category": "%category.javaLauncher%"
      },
      {
        "command": "java-launcher.searchAndRun",
        "title": "%command.searchAndRun.title%",
        "icon": "$(search)",
        "category": "%category.javaLauncher%"
      },
      {
        "command": "java-launcher.refresh",
        "title": "%command.refresh.title%",
        "icon": "$(refresh)"
      },
      {
        "command": "java-launcher.run",
        "title": "%command.run.title%",
        "icon": "$(play)"
      },
      {
        "command": "java-launcher.debug",
        "title": "%command.debug.title%",
        "icon": "$(debug-alt)"
      },
      {
        "command": "java-launcher.addToAggregated",
        "title": "%command.addToAggregated.title%",
        "icon": "$(add)"
      },
      {
        "command": "java-launcher.runAggregated",
        "title": "%command.runAggregated.title%",
        "icon": "$(run-all)"
      },
      {
        "command": "java-launcher.editAggregated",
        "title": "%command.editAggregated.title%",
        "icon": "$(edit)"
      },
      {
        "command": "java-launcher.removeFromAggregated",
        "title": "%command.removeFromAggregated.title%",
        "icon": "$(remove)"
      },
      {
        "command": "java-launcher.deleteAggregated",
        "title": "%command.deleteAggregated.title%",
        "icon": "$(trash)"
      },
      {
        "command": "java-launcher.setJmxRemoteEnabled",
        "title": "%command.setJmxRemoteEnabled.title%",
        "icon": "$(check)"
      },
      {
        "command": "java-launcher.setJmxRemoteDisabled",
        "title": "%command.setJmxRemoteDisabled.title%",
        "icon": "$(close)"
      },
      {
        "command": "java-launcher.setAllJmxRemoteEnabled",
        "title": "%command.setAllJmxRemoteEnabled.title%",
        "icon": "$(check-all)"
      },
      {
        "command": "java-launcher.setAllJmxRemoteDisabled",
        "title": "%command.setAllJmxRemoteDisabled.title%",
        "icon": "$(close-all)"
      },
      {
        "command": "java-launcher.manageRunningProcesses",
        "title": "%command.manageRunningProcesses.title%",
        "icon": "$(window)"
      },
      {
        "command": "java-launcher.jumpToSource",
        "title": "%command.jumpToSource.title%",
        "icon": "$(go-to-file)",
        "category": "%category.javaLauncher%"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "javaLauncher",
          "title": "%viewsContainers.activitybar.javaLauncher.title%",
          "icon": "$(rocket)"
        }
      ]
    },
    "views": {
      "javaLauncher": [
        {
          "id": "javaLauncherView",
          "name": "%views.javaLauncher.javaLauncherView.name%",
          "when": "workspaceFolderCount > 0"
        }
      ]
    },
    "menus": {
      "explorer/context": [
        {
          "when": "resourceExtname == .java",
          "command": "java-launcher.generateLaunchConfigs",
          "group": "1_modification"
        }
      ],
      "commandPalette": [
        {
          "command": "java-launcher.generateLaunchConfigs",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "java-launcher.scanJavaEntries",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "java-launcher.createAggregatedLaunch",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "java-launcher.manageAggregatedLaunch",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "java-launcher.executeAggregatedLaunch",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "java-launcher.debugAggregatedLaunch",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "java-launcher.showAllCommands",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "java-launcher.setSpringProfile",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "java-launcher.setAllSpringProfiles",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "java-launcher.setAllJmxRemoteStatus",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "java-launcher.searchAndRun",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "java-launcher.manageRunningProcesses",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "java-launcher.jumpToSource",
          "when": "workspaceFolderCount > 0"
        }
      ],
      "view/title": [
        {
          "command": "java-launcher.searchAndRun",
          "when": "view == javaLauncherView",
          "group": "navigation"
        },
        {
          "command": "java-launcher.manageRunningProcesses",
          "when": "view == javaLauncherView",
          "group": "navigation"
        },
        {
          "command": "java-launcher.showAllCommands",
          "when": "view == javaLauncherView",
          "group": "navigation"
        },
        {
          "command": "java-launcher.refreshTreeView",
          "when": "view == javaLauncherView",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "java-launcher.runJavaEntry",
          "when": "view == javaLauncherView && viewItem == springBootApp",
          "group": "inline"
        },
        {
          "command": "java-launcher.addToAggregatedConfig",
          "when": "view == javaLauncherView && viewItem == springBootApp",
          "group": "inline"
        },
        {
          "command": "java-launcher.setSpringProfile",
          "when": "view == javaLauncherView && viewItem == springBootApp",
          "group": "inline"
        },
        {
          "command": "java-launcher.jumpToSource",
          "when": "view == javaLauncherView && viewItem == springBootApp",
          "group": "inline"
        },
        {
          "command": "java-launcher.runJavaEntry",
          "when": "view == javaLauncherView && viewItem == javaApp",
          "group": "inline"
        },
        {
          "command": "java-launcher.addToAggregatedConfig",
          "when": "view == javaLauncherView && viewItem == javaApp",
          "group": "inline"
        },
        {
          "command": "java-launcher.jumpToSource",
          "when": "view == javaLauncherView && viewItem == javaApp",
          "group": "inline"
        },
        {
          "command": "java-launcher.runJavaEntry",
          "when": "view == javaLauncherView && viewItem == testClass",
          "group": "inline"
        },
        {
          "command": "java-launcher.addToAggregatedConfig",
          "when": "view == javaLauncherView && viewItem == testClass",
          "group": "inline"
        },
        {
          "command": "java-launcher.jumpToSource",
          "when": "view == javaLauncherView && viewItem == testClass",
          "group": "inline"
        },
        {
          "command": "java-launcher.runJavaEntry",
          "when": "view == javaLauncherView && viewItem == testMethod",
          "group": "inline"
        },
        {
          "command": "java-launcher.addToAggregatedConfig",
          "when": "view == javaLauncherView && viewItem == testMethod",
          "group": "inline"
        },
        {
          "command": "java-launcher.jumpToSource",
          "when": "view == javaLauncherView && viewItem == testMethod",
          "group": "inline"
        },
        {
          "command": "java-launcher.runAggregatedConfig",
          "when": "view == javaLauncherView && viewItem == aggregatedConfig",
          "group": "inline"
        },
        {
          "command": "java-launcher.manageAggregatedLaunch",
          "when": "view == javaLauncherView && viewItem == aggregatedConfigs",
          "group": "inline"
        },
        {
          "command": "java-launcher.setAllSpringProfiles",
          "when": "view == javaLauncherView && viewItem == javaEntries",
          "group": "inline"
        }
        ,
        {
          "command": "java-launcher.createAggregatedLaunch",
          "when": "view == javaLauncherView && viewItem == aggregatedConfigs",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "ts-loader": "^9.5.2",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  },
  "dependencies": {
    "glob": "^10.3.10",
    "fast-glob": "^3.3.2"
  }
}
